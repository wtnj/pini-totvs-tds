Static Function R019Proc()

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Envia controle para a funcao SETPRINT                        ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
While lContinua
	wnrel := SetPrint(cString,wnrel,cPerg,Titulo,cDesc1,cDesc2,cDesc3,.T.)
	
    If nLastKey == 27
		Return
    EndIf
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Verifica Posicao do Formulario na Impressora                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	SetDefault(aReturn,cString)
	
    If nLastKey == 27
		Return
    EndIf
    lContinua:=.T.
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ  Prepara regua de impressฦo                                  ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	tregs := LastRec()-Recno()+1

    _aCampos := {   {"NOME"     ,"C",40, 0} ,;
                    {"AGENCIA"  ,"C",40, 0} ,;
                    {"v_End"    ,"C",40, 0} ,;
                    {"BAIRRO"   ,"C",20, 0} ,;
                    {"MUN"      ,"C",20, 0} ,;
                    {"CEP"      ,"C",8 , 0} ,;
                    {"EST"      ,"C",2 , 2}}
	
	_cNome := CriaTrab(_aCampos,.t.)
	dbUseArea(.T.,, _cNome,"ETIQP",.F.,.F.)
	
    dbSelectArea('SF2')
    dbSetOrder(1)
    If !dbSeek(xFilial("SF2")+MV_PAR01, .F.)
		Return
    EndIf
	
	ProcRegua(RecCount())
	
    While !Eof() .and. SF2->F2_FILIAL == xFilial("SF2") .and. SF2->F2_DOC >= MV_PAR01 .and. SF2->F2_DOC <= MV_PAR02
		
		IncProc("Gerando Etiqueta da Nota "+SF2->F2_SERIE+SF2->F2_DOC)
		
		XNOTA    := SF2->F2_DOC
		MAGENCIA := ' '
		XSERIE   := SF2->F2_SERIE
		
        dbSelectArea("SD2")
        dbSetOrder(3)
        dbSeek(xFilial("SD2")+XNOTA+XSERIE)
        If SUBS(SD2->D2_PEDIDO,6,1)#'P'
            dbSelectArea("SF2")
            dbSkip()
            Loop
        EndIf
		XPEDIDO := SD2->D2_PEDIDO
		
        dbSelectArea("SC5")
        dbSetOrder(1)
        dbSeek(xFilial("SC5")+XPEDIDO)
		MTPTRANS := SC5->C5_TPTRANS
		XCLIENTE := SC5->C5_CLIENTE
		XLOJA    := SC5->C5_LOJACLI
		XCODAG   := SC5->C5_CODAG
		
        dbSelectArea("SA1")
        dbSetOrder(1)
        dbSeek(xFilial("SA1")+XCLIENTE+XLOJA)
		MNOME  := SA1->A1_NOME
        MEnd   := SA1->A1_End
		MBAIR  := SA1->A1_BAIRRO
		MMUNI  := SA1->A1_MUN
		MESTA  := SA1->A1_EST
		MCEP   := SA1->A1_CEP
		
        If SUBS(SA1->A1_EndCOB,1,1) == 'S'
            dbSelectArea("SZ5")
            dbSetOrder(1)
            If dbSeek(xFilial("SZ5")+XCLIENTE+XLOJA, .F.)
                MEnd   := SZ5->Z5_END
				MBAIR  := SZ5->Z5_BAIRRO
				MMUNI  := SZ5->Z5_CIDADE
				MESTA  := SZ5->Z5_ESTADO
				MCEP   := SZ5->Z5_CEP
            EndIf
        EndIf
		
        If MTPTrans $ "04/12"
        //If MTPTrans == "04"       - Alterado em 16/03/00 - By RC
            dbSelectArea("SA1")
            If dbSeek(xFilial("SA1")+XCODAG,.f.)    // +_clojaag) - ACRESCENTAR
				MAGENCIA := SA1->A1_NOME
            EndIf
            MEnd   := SA1->A1_End
			MBAIR  := SA1->A1_BAIRRO
			MMUNI  := SA1->A1_MUN
			MESTA  := SA1->A1_EST
			MCEP   := SA1->A1_CEP
			
        EndIf
		
        If MTPTRANS == '05' .OR. MTPTRANS == '09'
            dbSelectArea("SA1")
            If dbSeek(xFilial("SA1")+XCODAG, .F.)
				MAGENCIA := ' '
            EndIf
        EndIf
		
		GRAVA_TEMP()
        dbSelectArea("SF2")
        dbSkip()
    End
	IMPETIQ()
End

dbSelectArea("SC6")
Retindex("SC6")
dbSelectArea("SC5")
Retindex("SC5")
dbSelectArea("SA1")
Retindex("SA1")
dbSelectArea("SZ5")
Retindex("SZ5")

set devi to screen
If aRETURN[5] == 1
	Set Printer to
	dbcommitAll()
	ourspool(WNREL)
EndIf
MS_FLUSH()

dbSelectArea("ETIQP")
dbCloseArea()
fErase(_cNome+".DBF")

dbSelectArea(_cAlias)
dbSetOrder(_nIndex)
dbGoTo(_nReg)

Return
