	DbUseArea( .T., "TOPCONN", TcGenQry(,,cQuery), "JOBPINI", .T., .F. )
	DbSelectArea("JOBPINI")
	dbGotop()
	While !EOF() //loop nos pedidos
		//montar array do SC5 
		/*aHeader
		[01] -> C5_NUM
