{"NUMPED"  ,"C",6, 0} ,;
{"ITEM"    ,"C",2, 0} ,;
{"EDSUSP"  ,"N",4, 0} ,;
{"EDISA"   ,"C",6, 0} ,;
{"SITUAC"  ,"C",2, 0} ,;
{"EDINIC"  ,"N",4, 0} ,;
{"EDFIN"   ,"N",4, 0} ,;
{"EDVENC"  ,"N",4, 0} ,;
{"PRODUTO" ,"C",15,0} ,;
{"PARC"    ,"N",3, 0} ,;
{"PARCPG"  ,"N",3, 0} ,;
{"PARCVV"  ,"N",3, 0} ,;
